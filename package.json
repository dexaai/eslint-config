{
  "name": "@dexaai/eslint-config",
  "version": "1.0.1",
  "description": "Dexa ESLint config",
  "repository": "dexaai/eslint-config",
  "private": false,
  "license": "MIT",
  "main": "index.js",
  "files": [
    "README.md",
    "config",
    "index.js"
  ],
  "scripts": {
